#include <stdio.h>
#include <conio.h>


char pass [5] = "ahmed";
char *Ppass;

char passCheck [5];
char *PpassCheck;

int counter;
int main ()

{
	Ppass = &pass;
	PpassCheck = &passCheck;
	
	printf("Enter the password: ");
	scanf("%s",&passcheck);
	
	while (*Ppass == *PpassCheck)
	{
		count ++;
		Ppass ++;
		PpassCheck ++;
		
		if (count == 5) break;
	}
	
	if count (count == 5) printf("\ncorrect password\n");
	else printf("wrong password\n");
	
}